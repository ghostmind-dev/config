{
  "id": "ai",
  "version": "0.0.18",
  "name": "AI",
  "description": "Claude Code and MCP Proxy installation with configuration binding",
  "containerEnv": {
    "CLAUDE_CONFIG_DIR": "/home/vscode/.claude"
  },
  "mounts": [
    {
      "source": "${localEnv:RUN_PROJECT}/.creds/.claude",
      "target": "/home/vscode/.claude",
      "type": "bind"
    }
  ]
}
