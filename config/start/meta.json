{
  "id": "SJtfHX8zO2kl",
  "name": "start",
  "type": "app",
  "routines": {
    "init-mount": "hdiutil attach ${HOME}/projects.dmg",
    "init-inspector": "mcp-inspector",
    "init-chrome": "mcp-proxy --port=8081 npx chrome-devtools-mcp@latest",
    "tmux-init": "run tmux init start",
    "tmux-attach": "run tmux attach start",
    "tmux-terminate": "run tmux terminate start",
    "cloudfflared_m1_tcp": "cloudflared access tcp --hostname m1-tcp.ghostmind.app --url tcp://localhost:5083"
  },
  "tmux": {
    "sessions": [
      {
        "name": "start",
        "root": ".",
        "windows": [
          {
            "name": "routines",
            "layout": "grid",
            "grid": {
              "type": "two-by-two",
              "panes": [
                {
                  "name": "top-left",
                  "command": "run routine init-mount"
                },
                {
                  "name": "top-right",
                  "command": "run routine init-chrome"
                },
                {
                  "name": "bottom-left",
                  "command": "run routine init-inspector"
                },
                {
                  "name": "bottom-right",
                  "command": "run routine cloudfflared_m1_tcp"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}
